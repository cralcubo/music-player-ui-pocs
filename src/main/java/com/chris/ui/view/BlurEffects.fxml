<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Rectangle?>

<StackPane onMouseEntered="#onHover" onMouseExited="#onExit" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chris.ui.controller.BlurController">
   <children>
      <ImageView fx:id="viewer" fitHeight="460.0" fitWidth="460.0" pickOnBounds="true" preserveRatio="true" />
      <Rectangle fx:id="rectangle" arcHeight="5.0" arcWidth="5.0" fill="#ffffff" height="460.0" opacity="0.0" stroke="WHITE" strokeType="INSIDE" width="460.0" />
   </children>
</StackPane>
